{
    "name": "@SAFE-anwang/web3.js-safe4-plugin",
    "version": "0.0.1",
    "description": "A Web3.js 4.x Plugin for Interacting With Safe4 Smart Contracts",
    "main": "lib/index.js",
    "types": "lib/index.d.ts",
    "scripts": {
        "test": "yarn run test:node",
        "test:node": "mocha -r ts-node/register test/**/*.spec.ts ",
        "clean": "rm -rf lib",
        "lint": "eslint --ext .ts src/",
        "check-types": "tsc --noEmit",
        "prepublishOnly": "yarn run build",
        "prebuild": "yarn run clean",
        "build": "yarn run build:lib",
        "build:lib": "babel src -x .ts -d lib --source-maps"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/SAFE-anwang/web3.js-plugin-safe4.git"
    },
    "author": "lemengbin",
    "license": "Apache-2.0",
    "bugs": {
        "url": "https://github.com/SAFE-anwang/web3.js-plugin-safe4/issues"
    },
    "homepage": "https://github.com/SAFE-anwang/web3.js-plugin-safe4/blob/master/README.md",
    "devDependencies": {
        "@babel/cli": "^7.2.3",
        "@babel/core": "^7.3.3",
        "@babel/preset-env": "^7.3.1",
        "@babel/preset-typescript": "^7.3.3",
        "@babel/register": "^7.0.0",
        "@typechain/ethers-v5": "^11.1.1",
        "@types/bn.js": "^4.11.5",
        "@types/mocha": "^5.2.5",
        "@types/node": "^12.0.10",
        "@typescript-eslint/eslint-plugin": "^1.3.0",
        "@typescript-eslint/parser": "^1.3.0",
        "eslint": "5.16.0",
        "eslint-plugin-import": "^2.18.2",
        "mocha": "^6.1.4",
        "ts-loader": "^6.2.1",
        "ts-node": "^8.3.0",
        "typescript": "^5.1.6",
        "typechain": "^8.3.1",
        "webpack": "^4.41.2"
    },
    "dependencies": {
        "web3": ">= 4.0.2 < 5"
    }
}
